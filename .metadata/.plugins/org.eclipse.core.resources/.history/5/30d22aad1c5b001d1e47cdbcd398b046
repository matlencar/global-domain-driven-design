package br.com.fiap.base.to;

public class BicicletaTO {
	
	 //Variaveis
	 private int id;
	 private int aro;
	 private double quadro;
	 private double peso;
	 private double altura;
	 private String cor;
	 private String tipo;
	 private int marcha;
	
	 //Construtor vazio
	 public BicicletaTO() {}

	 //Construtor com parametros e variaveis
	public BicicletaTO(int id, int aro, double quadro, double peso, double altura, String cor, String tipo,
			int marcha) {
		this.id = id;
		this.aro = aro;
		this.quadro = quadro;
		this.peso = peso;
		this.altura = altura;
		this.cor = cor;
		this.tipo = tipo;
		this.marcha = marcha;
	}

	//Getters and Setters
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getAro() {
		return aro;
	}

	public void setAro(int aro) {
		this.aro = aro;
	}

	public double getQuadro() {
		return quadro;
	}

	public void setQuadro(double quadro) {
		this.quadro = quadro;
	}

	public double getPeso() {
		return peso;
	}

	public void setPeso(double peso) {
		this.peso = peso;
	}

	public double getAltura() {
		return altura;
	}

	public void setAltura(double altura) {
		this.altura = altura;
	}

	public String getCor() {
		return cor;
	}

	public void setCor(String cor) {
		this.cor = cor;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public int getMarcha() {
		return marcha;
	}

	public void setMarcha(int marcha) {
		this.marcha = marcha;
	}
}
