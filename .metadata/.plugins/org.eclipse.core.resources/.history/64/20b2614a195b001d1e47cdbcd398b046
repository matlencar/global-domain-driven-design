package br.com.fiap.base.to;

public class CorridaTO {

	//Variaveis
	private int id;
	private double corrida;
	private double tempo;
	
	//Construtor vazio
	public CorridaTO() {}

	//Construtor com parametros e variaveis
	public CorridaTO(int id, double corrida, double tempo) {
		this.id = id;
		this.corrida = corrida;
		this.tempo = tempo;
	}

	//Getters and Setters
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public double getCorrida() {
		return corrida;
	}

	public void setCorrida(double corrida) {
		this.corrida = corrida;
	}

	public double getTempo() {
		return tempo;
	}

	public void setTempo(double tempo) {
		this.tempo = tempo;
	}
}
