public class TelefoneUsuarioBO {
	
	private TelefoneUsuarioDao telefoneUsuarioDao;
	private Connection conexao;
	
	public TelefoneUsuarioBO(Connection conexao) {
		
		this.telefoneUsuarioDao = new EnderecoUsuarioDao(conexao);
	}
}
